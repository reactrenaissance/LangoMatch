{
  "expo": {
    "name": "LangoMatch",
    "slug": "LangoMatch",
    "version": "2.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends.",
          "cameraPermission": "Allow LangoMatch to access your camera."
        }
      ]
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.reactrenaissance.LangoMatch",
      "buildNumber": "1",
      "infoPlist": {
        "CFBundleShortVersionString": "2.0.1",
        "CFBundleVersion": "1"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.reactrenaissance.LangoMatch",
      "googleServicesFile": "./google-services.json",
      "versionCode": 1 
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "2e9471d0-471c-4861-990e-739299b6ae60"
      }
    }
  }
}
