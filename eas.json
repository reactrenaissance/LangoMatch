{
  "cli": {
    "version": ">= 9.0.3"
  },
  "build": {
    "release": {
      "developmentClient": false,
      "distribution": "store",
      "android": {
        "buildType": "app-bundle",
        "preBuildCommands": [
          "./log-env.sh"
        ],
        "env": {
          "GOOGLE_SERVICES_JSON": "GOOGLE_SERVICES_JSON@secret"
        }
      },
      "ios": {
        "image": "latest",
        "env":{
        "googleServicesFile": "GOOGLE_SERVICES_INFO@secret"
      }
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {}
  },
  "submit": {
    "production": {}
  }
}

